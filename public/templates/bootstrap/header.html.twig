<head>
    <meta charset="{{ config.charset }}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{ title }}</title>

    <!-- Includes -->
    <link href="{{ config.vendor }}/twbs/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="/{{ config.tpl }}/css/custom.css" rel="stylesheet">
    <link href="/{{ config.tpl }}/js/jasny-bootstrap/css/jasny-bootstrap.min.css" rel="stylesheet">

    {% if redirect %}
        <meta http-equiv="refresh" content="{{ redirect.time }}; url={{ redirect.target}}" />
    {% endif %}
</head>